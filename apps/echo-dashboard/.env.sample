# Base URL for the Echo dashboard backend API
NEXT_PUBLIC_ECHO_API_BASE_URL=http://localhost:5050

# Token used by scripts/gen_codex_registry.py (set in GitHub Actions secrets)
GITHUB_TOKEN=replace-with-github-token

# Neon/Postgres connection string for storing attestations
NEON_DATABASE_URL=postgres://user:password@host:5432/database

# Assistant tool integration (if connected to an LLM gateway)
OPENAI_API_KEY=replace-with-openai-key

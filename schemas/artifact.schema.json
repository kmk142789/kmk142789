{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "artifact.schema.json",
  "title": "Colossus Artifact",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "$schema",
    "version",
    "id",
    "type",
    "cycle",
    "seed",
    "created_at",
    "sha256",
    "parents",
    "meta"
  ],
  "properties": {
    "$schema": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "id": {
      "type": "string",
      "minLength": 1
    },
    "type": {
      "type": "string",
      "minLength": 1
    },
    "cycle": {
      "type": "integer",
      "minimum": 0
    },
    "seed": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "sha256": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$"
    },
    "parents": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "meta": {
      "type": "object"
    }
  }
}
